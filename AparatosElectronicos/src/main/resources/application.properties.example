spring.application.name=AparatosElectronicos
spring.datasource.url=jdbc:mysql://[IP ADDRESS CONNECTION]:[PORT MYSQL]/[DATABASE NAME]
spring.datasource.username=[DATABASE USER]
spring.datasource.password=[DATABASE PASSWORD]
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver


# Configuraci√≥n de Redis
spring.cache.type=redis
spring.data.redis.host=[DATABASE HOST]
spring.data.redis.port=[DATABASE PORT]